<h2 class="display-1">Dam Status</h2>
<div class="table-striped table-hover table-responsive">
    <table class="table" align="center">
        <thead class="container">
            <tr>
                <th>#</th>
                <th>Dam</th>
                <th>Current Level</th>
                <th>à®¨Inflow<span class="glyphicon glyphicon-triangle-top text-success"></span></th>
                <th>Outflow<span class="glyphicon glyphicon-triangle-bottom text-danger"></span></th>
                <th>Date</th>
            </tr>
        </thead>
        <tbody>	
            {{#each levels}}<tr>
                    <td><a href="/level/{{this.damId}}"><span class="glyphicon glyphicon-list-alt"></span></a></span></td>
                    <td><a href="/level/{{this.damId}}">{{this.name}}</a></td>
                    <td><strong>{{this.level }}</strong><span class="text-success"> <small>feet</small></td>
                    <td><strong>{{this.inflow }}</strong><small></small>cusecs</small></td>
                    <td><strong>{{this.outflow }}</strong><small>cusecs</small></td>
                    <td><strong>{{this.updated }}</strong></td>
                </tr>
            {{/each}}   
        </tbody>
    </table>
</div>